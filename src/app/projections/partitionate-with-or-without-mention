fromStream('github-events')
.whenAny(function(state, ev) {
    var re = new RegExp("[A-Z]{1,2}-[0-9]+", "");
    var matches = ev.data.commit.message.match(re);
    if(matches && 0 < matches.length){
        linkTo('mention-with', ev);
    }
    else {
        linkTo('mention-without', ev);
    }
});